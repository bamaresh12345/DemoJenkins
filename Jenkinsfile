pipeline {

  agent any

  stages{

    stage("BUILD")
    {
      steps {
           echo 'BUILD  - Amar Stage'
      }    
     
    }

     stage("TEST Amar Stage")
    {
      steps {
                   echo 'TEST Amar Stage'
      
      }    
      
    }


     stage("DEPLOY Amar Stage")
    {
      steps {
           echo 'DEPLOY Amar Stage'
      
      }    
      
    }
  }
}
